{
	"name": "kha",
	"displayName": "Kha",
	"description": "Kha support",
	"version": "0.0.1",
	"publisher": "ktx",
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:kha.init",
		"onCommand:kha.compile"
	],
	"main": "./extension",
	"contributes": {
		"commands": [{
			"command": "kha.init",
			"title": "Init Kha Project"
		},
		{
			"command": "kha.compile",
			"title": "Compile Kha Project"
		}],
		"keybindings": [{
				"command": "kha.compile"
				, "key": "ctrl+b"
				, "mac": "cmd+b"
		}]
	},
	"devDependencies": {
		"vscode": "0.10.x"
	}
}